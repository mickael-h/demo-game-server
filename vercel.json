{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/bin/www.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/bin/www.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "startCommand": "npm start"
}
